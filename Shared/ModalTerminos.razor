@using MudBlazor

<MudDialog>
    <DialogContent>
        <iframe src="@Url" width="100%" height="300px" frameborder="0"></iframe>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Text" OnClick="() => MudDialog?.Close()">Cerrar</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] 
    MudDialogInstance? MudDialog { get; set; }

    [Parameter] 
    public string Url { get; set; } = "";
}
